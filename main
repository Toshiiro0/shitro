				AnimationPack.TextColor3 = Color3.fromRGB(0, 0, 0)
				AnimationPack.TextSize = 18.000
				AnimationPack.MouseButton1Click:Connect(function()
					if (Animations.Visible == false) then
						Animations.Visible = true;
					end;
				end);

